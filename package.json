  },
  "homepage": "https://github.com/alandooley/reta#readme",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "playwright": "^1.55.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.916.0",
    "@aws-sdk/credential-providers": "^3.916.0",
    "@aws-sdk/lib-dynamodb": "^3.916.0",
